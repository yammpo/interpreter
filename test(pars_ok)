<PROGRAM>
	<VARDECLARATION>
		<VAR = a> <TYPE> INT </TYPE> </VAR>
		<VAR = a> <TYPE> INT </TYPE> </VAR>
		<VAR = a> <TYPE> INT </TYPE> </VAR>
	</VARDECLARATION>
	<VARDECLARATION>
		<VAR = a> <TYPE> INT </TYPE> </VAR>
		<VAR = a> <TYPE> INT </TYPE> </VAR>
		<VAR = a> <TYPE> INT </TYPE> </VAR>
	</VARDECLARATION>
	<FUNC name = factorial>
		<VARDECLARATION>
			<VAR = b> <TYPE> INT </TYPE> </VAR>
		</VARDECLARATION>
		<VARDECLARATION>
			<VAR = b> <TYPE> INT </TYPE> </VAR>
			<VAR = b> <TYPE> INT </TYPE> </VAR>
		</VARDECLARATION>
	</FUNC>
	<FUNC name = main>
		<ASSIGN> 
			<VALUE> 5 </VALUE> 
			<TO> <VAR name = a /VAR> </TO>
		</ASSIGN>
		<ASSIGN> 
			<VALUE> <VAR name = b /VAR> </VALUE> 
			<TO> <VAR name = a /VAR> <VAR name = c /VAR> <VAR name = ds /VAR> </TO>
		</ASSIGN>
		<CALL> factorial </CALL>
	</FUNC>
</PROGRAM>
